{
	"name": "@natoboram/bug-report-rollup",
	"version": "0.0.0",
	"description": "A bug report reproduction for Rollup",
	"keywords": [
		"rollup"
	],
	"homepage": "https://github.com/NatoBoram/bug-report-rollup",
	"bugs": {
		"url": "https://github.com/NatoBoram/bug-report-rollup/issues"
	},
	"license": "Unlicense",
	"author": {
		"name": "Nato Boram",
		"url": "https://github.com/NatoBoram"
	},
	"funding": [
		{
			"type": "patreon",
			"url": "https://www.patreon.com/NatoBoram"
		},
		{
			"type": "individual",
			"url": "https://paypal.me/NatoBoram/5"
		}
	],
	"files": [
		"dist",
		"!dist/**/*.test.*"
	],
	"main": "dist/index.js",
	"repository": "github:NatoBoram/bug-report-rollup",
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf dist docs node_modules tsconfig.tsbuildinfo",
		"dev": "tsx ./src/main.ts",
		"docs": "typedoc",
		"format": "prettier --write .",
		"lint": "eslint . && markdownlint-cli2 \"**/*.md\" && prettier --check .",
		"lint:fix": "eslint --fix --quiet .; markdownlint-cli2 \"**/*.md\" --fix; prettier --list-different --write .",
		"start": "node dist/main.js",
		"test": "vitest",
		"rollup": "rollup -c rollup.config.js"
	},
	"devDependencies": {
		"@eslint/js": "^9.6.0",
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/node": "^20.14.9",
		"eslint": "^9.6.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.8.0",
		"markdownlint-cli2": "^0.13.0",
		"prettier": "^3.3.2",
		"rollup": "^4.18.0",
		"tslib": "^2.6.3",
		"tsx": "^4.16.2",
		"typedoc": "^0.26.3",
		"typescript": "^5.5.3",
		"typescript-eslint": "^7.15.0",
		"vitest": "^2.0.5"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"types": "dist/index.d.ts",
	"module": "dist/index.js"
}
